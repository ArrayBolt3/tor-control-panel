[Unit]
Description=Make Sure Torrc Files Exist For tor Control Panel
Documentation=https://github.com/Whonix/anon-gw-anonymizer-config
ConditionPathExists=!/var/run/qubes/this-is-templatevm
Before=tor.service

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/lib/other-anonymizer-config/torrc-parser

[Install]
WantedBy=multi-user.target
